{
  "@context": {
    "ex": "https://example.org/crypto-kg#",
    "schema": "https://schema.org/",
    "prov": "http://www.w3.org/ns/prov#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "Strategy": "ex:Strategy",
    "Indicator": "ex:Indicator",
    "IndicatorUse": "ex:IndicatorUse",
    "Rule": "ex:Rule",
    "EvidenceSpan": "ex:EvidenceSpan",
    "Document": "ex:Document",
    "ExtractionRun": "ex:ExtractionRun",
    "Token": "ex:Token",
    "Whitepaper": "ex:Whitepaper",
    "Agent": "ex:Agent",

    "name": "schema:name",
    "description": "schema:description",
    "signature": "ex:signature",
    "timeframe": "ex:timeframe",
    "universe": { "@id": "ex:universe", "@container": "@set" },
    "hasRule": { "@id": "ex:hasRule", "@container": "@set" },
    "usesIndicator": { "@id": "ex:usesIndicator", "@container": "@set" },
    "hasSource": { "@id": "ex:hasSource", "@container": "@set" },
    "direction": "ex:direction",
    "operator": "ex:operator",
    "left": "ex:left",
    "right": "ex:right",
    "params": { "@id": "ex:params", "@container": "@index" },
    "text": "schema:text",
    "start": { "@id": "ex:start", "@type": "xsd:integer" },
    "end": { "@id": "ex:end", "@type": "xsd:integer" },
    "page": { "@id": "ex:page", "@type": "xsd:integer" },
    "url": "schema:url",
    "doi": "schema:identifier",
    "sourceOf": "ex:sourceOf",
    "generatedBy": "prov:wasGeneratedBy",
    "generatedAt": "prov:generatedAtTime",
    "confidence": "ex:confidence",
    "backtestHint": "ex:backtestHint",
    "altLabel": "skos:altLabel",
    "prefLabel": "skos:prefLabel",

    "symbol": "ex:symbol",
    "describesToken": "ex:describesToken",
    "authoredBy": "ex:authoredBy",
    "cites": "ex:cites",
    "derivedFrom": "ex:derivedFrom",
    "targetsToken": "ex:targetsToken",
