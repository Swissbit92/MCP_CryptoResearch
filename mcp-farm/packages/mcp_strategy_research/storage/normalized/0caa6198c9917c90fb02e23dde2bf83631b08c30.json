  "universe": [
    "BTCUSDT"
  ],
  "timeframe": "1h",
  "indicators": [
    {
      "name": "RSI",
      "params": {
        "window": 14
      }
    },
    {
      "name": "MACD",
      "params": {
        "fast": 12,
        "slow": 26,
        "signal": 9
      }
    }
