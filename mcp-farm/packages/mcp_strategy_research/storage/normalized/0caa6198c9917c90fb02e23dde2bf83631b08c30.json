    "RSI crosses below 70",
    "or trailing_stop(ATR, multiple=2.0)"
  ],
  "position_sizing": {
    "type": "fixed_risk",
    "risk_pct": 1.0
  },
  "defaults": {
    "stop": {
      "atr_mult": 2.0
    },
    "take_profit": null
  },
  "backtest_hints": {
    "warmup_bars": 200
  },
